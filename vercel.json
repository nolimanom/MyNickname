{
  "version": 2,
  "routes": [
    { "src": "/profile.js", "dest": "/Pages/profile.js" },
    { "src": "/Users/(.*)", "dest": "/Users/$1" },
    { "src": "/create", "dest": "/Pages/create.html" },
    { "src": "/(.*)", "dest": "/Pages/Index.html" }
  ],
  "headers": [
    {
      "source": "/Users/(.*).json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/profile.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
