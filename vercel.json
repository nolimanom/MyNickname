{
  "version": 2,
  "rewrites": [
    { "source": "/profile.js", "destination": "/Pages/profile.js" },
    { "source": "/Users/(.*)", "destination": "/Users/$1" },
    { "source": "/create", "destination": "/Pages/create.html" },
    { "source": "/(.*)", "destination": "/Pages/Index.html" }
  ],
  "headers": [
    {
      "source": "/Users/(.*).json",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=60, stale-while-revalidate=600" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/profile.js",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=3600, stale-while-revalidate=86400" }
      ]
    }
  ]
}
