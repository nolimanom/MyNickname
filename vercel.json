{
  "version": 2,
  "builds": [
    {
      "src": "Pages/profile.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/profile",
      "destination": "/Pages/profile.js"
    },
    {
      "source": "/create",
      "destination": "/Pages/create.html"
    },
    {
      "source": "/",
      "destination": "/Pages/index.html"
    }
  ]
}
